
@{
    ViewBag.Title = "Find Employee";
}
@model MVCClient.Models.EmployeeModel
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Find Employee</title>
</head>
<body>
    <h2>Find Employee By Id</h2>
    <div>
        @using (Html.BeginForm("SubmitFindEmployeeById", "Home", FormMethod.Post))
        {
            <form>
                <div>
                    @Html.LabelFor(model => model.Id)
                    @Html.TextBoxFor(model => model.Id)
                    @* @Html.Hidden("Emplname", Model.Empname)*@
                    <input type="submit" id="submitId" value="submit" />

                </div>
            </form>
        }
    </div>

</body>
</html>
